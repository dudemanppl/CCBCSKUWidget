{
  "root": true,
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "jsx-a11y/href-no-hash": ["off"],
    "no-undef": "off", 
    "no-shadow": "off",
    "no-param-reassign": "off",
    "no-restricted-syntax": "off",
    "no-use-before-define": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
